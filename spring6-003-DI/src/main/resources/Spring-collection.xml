<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

    <bean id="personBean" class="com.lml.spring6.bean.Person">
        <property name="properties">
            <props>
                <prop key="driver">com.mysql.cj.jdbc.Driver</prop>
                <prop key="url">jdbc:mysql://localhost:3306/spring6</prop>
                <prop key="username">root</prop>
                <prop key="password">123456</prop>
            </props>
        </property>
        <property name="phones">
            <map>
                <entry key="1" value="110"></entry>
                <entry key="2" value="120"></entry>
                <entry key="3" value="119"></entry>
                <entry key="4" value="114"></entry>
                <entry key="5" value="198"></entry>
            </map>
        </property>
        <property name="address">
            <set>
                <value>zhangsan</value>
                <value>lisi</value>
                <value>lisi</value>
                <value>lisi</value>
                <value>lisi</value>
            </set>
        </property>
        <property name="names">
            <list>
                <value>nanchang</value>
                <value>nanchang</value>
                <value>nanchang</value>
                <value>jiangxi</value>
            </list>
        </property>
    </bean>

</beans>